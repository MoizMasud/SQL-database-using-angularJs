<!DOCTYPE html>
<html>
<head>
  <!-- Standard Meta -->
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-COMPATIBLE" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>Xfit Home</title>
  <!-- Stylesheets -->
  <link href="lib/semantic/semantic.css" rel="stylesheet" type="text/css">
  <link href="css/login.css" rel="stylesheet" type="text/css">
</head>
<body>
<div id="wrapper" class="ui middle aligned center aligned grid">
  <div class="column" style="max-width: 450px;">
    <h1 class="ui green header">Xfit</h1>

    <div ng-controller="dashboardController">
      <div>
        <% for (var i = 0; i < mes.length; i++) { %>
          <p><%= mes[i].routineName +" contains: "+ mes[i].exerciseName + " | " + mes[i].description %></p>
        <% } %>
      </div>
      <div>

      </div>

    </div>
    <div class="ui message">

      <a href="/dashboard">Done</a>
    </div>

  </div>

  <!-- JavaScript -->
  <script src="lib/jquery/jquery.js" type="text/javascript"></script>
  <script src="lib/semantic/semantic.js" type="text/javascript"></script>
</body>
</html>
